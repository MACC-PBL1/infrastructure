[SERVICE]
    Flush        5
    Daemon       Off
    Log_Level    info
    Parsers_File parsers.conf

[INPUT]
    Name              tail
    Path              /home/admin/zeek-logs/zeek/flowmeter.log
    Parser            json
    Tag               zeek.flowmeter
    Refresh_Interval  5
    Read_from_Head    False

[INPUT]
    Name              tail
    Path              /home/admin/zeek-logs/zeek/conn.log
    Parser            json
    Tag               zeek.conn
    Refresh_Interval  5
    Read_from_Head    False

[OUTPUT]
    Name              kinesis_firehose
    Match             zeek.flowmeter
    region            us-east-1
    delivery_stream   pi-infra-development-user-zeekflowmeter-firehose
    time_key          ts
    time_key_format   %s.%L

[OUTPUT]
    Name              kinesis_firehose
    Match             zeek.conn
    region            us-east-1
    delivery_stream   pi-infra-development-user-zeek-firehose
    time_key          ts
    time_key_format   %s.%L
